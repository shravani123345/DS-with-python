   "source": [
    "def div(m,n):\n",
    "    divi = 0\n",
    "    ndivi = 0\n",
    "    for i in range(1,m+1):\n",
    "        if i % n == 0:\n",
    "            divi = divi+i\n",
    "        else:\n",
    "            ndivi = ndivi+i\n",
    "    if ndivi>divi:\n",
    "        print(ndivi - divi)\n",
    "    else:\n",
    "        print(divi - ndivi)  "
   ]
